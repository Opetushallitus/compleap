    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "jest": "^24.1.0",
    "jest-puppeteer": "^4.0.0",
    "puppeteer": "^1.12.2",
    "react-test-renderer": "^16.8.2",
    "webpack": "^4.29.3",
