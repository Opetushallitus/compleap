  },
  "scripts": {
    "build:dev": "./node_modules/.bin/webpack --config webpack.dev.js",
    "build:prod": "NODE_ENV=production ./node_modules/.bin/webpack --config webpack.prod.js",
    "start:dev": "./node_modules/.bin/webpack-dev-server --config webpack.dev.js",
    "test": "./node_modules/.bin/concurrently \"npm run test:ui\" \"npm run test:component\"",
    "test:ui": "npm run build:prod && JEST_ENV=jest-environment-puppeteer ./node_modules/.bin/jest --testPathPattern=page.spec.js",
    "test:component": "JEST_ENV=jsdom ./node_modules/.bin/jest --testPathPattern=test.js"
  },
  "dependencies": {
    "@babel/runtime": "^7.3.1",
    "bacon.atom": "^5.0.5",
